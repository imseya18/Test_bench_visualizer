{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "TestBench Visualizer",
  "version": "0.1.0",
  "identifier": "com.banc-test-mvp.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://10.69.4.50:1421",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "TestBench_Visualizer",
        "width": 1100,
        "height": 800,
        "maximized": true,
        "decorations": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
